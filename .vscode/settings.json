{
  "yaml.customTags": [
    "!@and sequence",
    "!@array sequence",
    "!@arraylike mapping",
    "!@compare mapping",
    "!@eval mapping",
    "!@exist",
    "!@object mapping",
    "!@or sequence",
    "!@query",
    "!@regexp",
    "!@trycatch mapping",
    "!$concat sequence",
    "!$convert mapping",
    "!$eval mapping",
    "!$mock",
    "!$query",
    "!$randstr"
  ],
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    {
      "language": "typescript",
      "autoFix": true
    },
  ],
  "[javascript]": {
    "editor.formatOnSave": true,
  },
  "[typescript]": {
    "editor.formatOnSave": true,
  },
  "typescript.format.insertSpaceAfterTypeAssertion": false
}