{
  "name": "template",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "lint": "eslint . --ext js,ts",
    "build": "tsc",
    "clean": "rimraf dist",
    "sync:models": "node ./scripts/sync-models.js db.sql src/models",
    "sync:apitype": "node ./scripts/sync-apitype.js api.jsona",
    "sync:apitype:manage": "node ./scripts/sync-apitype.js apiManage.jsona",
    "sync:apitype:inner": "node ./scripts/sync-apitype.js apiInner.jsona",
    "sync:handlers": "node ./scripts/sync-handlers.js api.jsona handlers",
    "sync:handlers:manage": "node ./scripts/sync-handlers.js apiManage.jsona handlersManage",
    "sync:handlers:inner": "node ./scripts/sync-handlers.js apiInner.jsona handlersInner",
    "sync:api": "npm run sync:apitype && npm run sync:handlers",
    "sync:api:manage": "npm run sync:apitype:manage && npm run sync:handlers:manage",
    "sync:api:inner": "npm run sync:apitype:inner && npm run sync:handlers:inner",
    "test": "apitest tests --ci"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.1.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/ioredis": "^4.28.1",
    "@types/koa": "^2.13.4",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.7",
    "bcryptjs": "^2.4.3",
    "ioredis": "^4.28.0",
    "jsona-openapi-js": "^0.3.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mysql2": "^2.3.2",
    "p-event": "^5.0.1",
    "sequelize": "6",
    "stoppable": "^1.1.0",
    "use-openapi": "0.4.0",
    "use-services": "^0.2.8",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@sigodenjs/apitest": "^0.11.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/koa__router": "^8.0.9",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "change-case": "^4.1.2",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sql-ddl-to-json-schema": "^4.0.6",
    "ts-node": "^10.4.0",
    "typegen-openapi": "^0.1.19",
    "typescript": "^4.4.4"
  }
}
