{
  "name": "template2",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon",
    "lint": "eslint . --ext js --ext ts",
    "build": "tsc",
    "clean": "rimraf dist",
    "sync:models": "node ./scripts/sync-models.js db.sql src/models",
    "sync:apitype": "node ./scripts/sync-apitype.js api.jsona",
    "sync:apitype:manage": "node ./scripts/sync-apitype.js apiManage.jsona",
    "sync:apitype:inner": "node ./scripts/sync-apitype.js apiInner.jsona",
    "sync:handlers": "node ./scripts/sync-handlers.js api.jsona handlers",
    "sync:handlers:manage": "node ./scripts/sync-handlers.js apiManage.jsona handlersManage",
    "sync:handlers:inner": "node ./scripts/sync-handlers.js apiInner.jsona handlersInner",
    "sync:api": "npm run sync:apitype && npm run sync:handlers",
    "sync:api:manage": "npm run sync:apitype:manage && npm run sync:handlers:manage",
    "sync:api:inner": "npm run sync:apitype:inner && npm run sync:handlers:inner",
    "test": "npm run htte",
    "htte": "htte htte/_config.yaml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@alicloud/pop-core": "^1.7.10",
    "@types/ioredis": "^4.17.7",
    "@types/koa": "^2.11.6",
    "@types/lodash": "^4.14.164",
    "@types/node": "^14.14.7",
    "ioredis": "^4.19.2",
    "jsona-openapi-js": "^0.1.16",
    "jsonwebtoken": "^8.5.1",
    "kcors": "^2.2.2",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.0.0",
    "koa-response-time": "^2.1.0",
    "koa-router": "^10.0.0",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "mysql2": "^2.2.5",
    "p-event": "^4.2.0",
    "sequelize": "6",
    "stoppable": "^1.1.0",
    "use-openapi": "^0.3.6",
    "use-services": "^0.2.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "change-case": "^4.1.1",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "htte-cli": "^0.6.1",
    "htte-plugin-builtin": "^0.6.1",
    "htte-reporter-cli": "^0.4.8",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "sql-ddl-to-json-schema": "^4.0.4",
    "ts-node": "^9.0.0",
    "typegen-openapi": "^0.1.18",
    "typescript": "^4.0.5"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
